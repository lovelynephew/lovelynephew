<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="team.lovelynephew.kidsmall.domain.product.ProductinfoRepository">
  
  <select id="product" parameterType="Integer" resultType="team.lovelynephew.kidsmall.domain.product.Product">
	SELECT *
	FROM product
	WHERE product.prd_code= #{prdCode};
  </select>
  
  
  </mapper>
